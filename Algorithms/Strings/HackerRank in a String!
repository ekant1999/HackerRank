def hackerrankInString(str):
    h = "hackerrank"
    pointer = 0
    for s in str:
        if s == h[pointer]:
            pointer += 1
            continue
    if pointer == len(h): 
        return "YES"
    return "NO"
x=int(input())
for i in range(2):
    s=input()
    op= hackerrankInString(s)
    print(op)
